.modal.fade.in{tabindex: -1, role: :dialog, id: "vacation_reporting_modal#{'_edit' unless local_assigns[:edit].nil? || local_assigns[:edit] == false }"}
  .modal-dialog
    .modal-content
      .modal-header
        %button.close{type: 'button', 'data-dismiss' => 'modal', 'aria-hidden' => 'true'} x
        %h4.modal-title#modal_label_2
          %span Vacation Reporting
      .modal-body= render partial: 'calendar/report_form', locals: {edit: local_assigns[:edit]}
